{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "unitytaurigame",
  "version": "0.1.0",
  "identifier": "com.example.unitytaurigame",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "unitytaurigame",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": {
        "default-src": "'self' 'unsafe-inline' blob: data: filesystem: tauri: http://localhost:*",
        "script-src": "'self' 'unsafe-eval' 'unsafe-inline' blob: data: filesystem: tauri:",
        "style-src": "'self' 'unsafe-inline' http://localhost:*",
        "img-src": "'self' data: blob: http://localhost:*",
        "connect-src": "'self' blob: data: http://localhost:*",
        "font-src": "'self' data: http://localhost:*",
        "frame-src": "'self' http://localhost:*",
        "child-src": "'self' http://localhost:*",
        "worker-src": "'self' blob: http://localhost:*",
        "media-src": "'self' blob: http://localhost:*"
      }
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
